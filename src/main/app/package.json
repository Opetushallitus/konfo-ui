{
  "name": "konfo-ui",
  "version": "0.1.0",
  "homepage": "/konfo",
  "private": true,
  "proxy": "https://untuvaopintopolku.fi",
  "scripts": {
    "prepare": "cd ../../../ && husky install",
    "start": "craco start",
    "build": "craco build",
    "build:test": "cross-env REACT_APP_CYPRESS=true craco build",
    "test": "craco test --env=jsdom",
    "serve:test": "node test-server.js",
    "cypress:run": "cross-env NODE_OPTIONS=\"--max-http-header-size=10000000\" cypress run -b chrome",
    "cypress:run:headless": "npm run cypress:run -- --headless",
    "cypress:open": "cross-env NODE_OPTIONS=\"--max-http-header-size=10000000\" cypress open -b chrome",
    "test:integration": "cross-env CYPRESS_RETRIES=3 NODE_ENV=test REACT_APP_CYPRESS=true PORT=3005 start-server-and-test serve:test http://localhost:3005/konfo cypress:run:headless",
    "test:ci": "cross-env CI=true npm run test && cross-env CI=true npm run test:integration",
    "lint": "eslint src/ cypress/",
    "lint:fix": "eslint src/ cypress/ --fix",
    "update-mocks": "node ./node_modules/kto-ui-common/scripts/update-mocks.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "dompurify": "^2.3.6",
    "i18next": "^21.6.4",
    "i18next-http-backend": "^1.4.1",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.5",
    "material-ui-flat-pagination": "^4.1.2",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js#9fc4edaa04c10fa974f124b5c228c41c2c6f0bc5",
    "oskari-rpc": "^2.1.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^11.15.2",
    "react-query": "^3.34.7",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^2.4.3",
    "react-select": "^5.2.1",
    "react-truncate-markup": "^5.1.0",
    "react-use": "^17.3.2",
    "redux": "^4.1.2",
    "remove-markdown": "^0.3.0",
    "stacktrace-js": "^2.0.2",
    "typeface-open-sans": "^1.1.13",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@craco/craco": "^6.4.3",
    "@cypress/snapshot": "^2.1.6",
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.178",
    "@types/markdown-to-jsx": "^7.0.1",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-router-hash-link": "^2.4.4",
    "@types/testing-library__cypress": "^5.0.9",
    "@types/testing-library__jest-dom": "^5.14.2",
    "babel-plugin-lodash": "^3.3.4",
    "cosmiconfig": "^7.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.2.0",
    "husky": "^8.0.1",
    "kto-ui-common": "github:opetushallitus/kto-ui-common#9e49bd2d3c5b956c5cbcb64d8d133276248ff21f",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "react-scripts": "^4.0.1",
    "source-map-explorer": "^2.5.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^#(.*)$": "<rootDir>$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
