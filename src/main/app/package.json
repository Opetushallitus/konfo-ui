{
  "name": "konfo-ui",
  "version": "0.1.0",
  "homepage": "/konfo",
  "private": true,
  "scripts": {
    "prepare": "cd ../../../ && husky install",
    "start": "vite dev",
    "build": "vite build",
    "preview:watch": "vite build --watch & npm run serve:test",
    "test": "cross-env NODE_ENV=test vitest",
    "serve:test": "node test-server.js",
    "test:integration": "start-server-and-test serve:test http://localhost:3005/konfo playwright",
    "test:ci": "cross-env CI=true npm run test && cross-env CI=true npm run test:integration",
    "playwright": "playwright test",
    "playwright:ui": "playwright test --ui",
    "lint": "eslint ./src ./playwright --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "analyze": "vite-bundle-visualizer",
    "tsc": "tsc",
    "cf-content-types": "cf-content-types-generator -s $CONTENTFUL_SPACE_ID -t $CONTENTFUL_MANAGEMENT_API_TOKEN -e $CONTENTFUL_ENVIRONMENT_ID -X -o ./src/types/contentful",
    "contentful:typegen": "dotenv-run-script .env.local -- cf-content-types",
    "konfo-backend:typegen": "npx openapi-typescript http://localhost:3006/konfo-backend/swagger.yaml -o ./src/types/konfo-backend.d.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.5",
    "@mui/styles": "^5.14.1",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.7.8",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.5",
    "html-react-parser": "^4.2.2",
    "i18next": "^23.2.11",
    "i18next-http-backend": "^2.2.1",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.2.1",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js#9fc4edaa04c10fa974f124b5c228c41c2c6f0bc5",
    "oskari-rpc": "^2.1.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-i18next": "^13.0.2",
    "react-inlinesvg": "^3.0.2",
    "react-player": "^2.14.1",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "react-select": "^5.7.4",
    "react-transition-group": "^4.4.5",
    "react-truncate-markup": "^5.1.2",
    "react-use": "^17.4.0",
    "redux": "^4.2.1",
    "remove-markdown": "^0.5.0",
    "stacktrace-js": "^2.0.2",
    "ts-pattern": "^5.0.3",
    "tslib": "^2.6.0",
    "typeface-open-sans": "^1.1.13",
    "victory": "^36.6.11",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@material-design-icons/svg": "^0.14.10",
    "@optimize-lodash/rollup-plugin": "^4.0.3",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/dompurify": "^3.0.2",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/node": "^16.18.36",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/testing-library__jest-dom": "^5.14.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "browserslist-to-esbuild": "^1.2.0",
    "cf-content-types-generator": "^2.12.2",
    "contentful": "^10.3.5",
    "cross-env": "^7.0.3",
    "dotenv-run-script": "^0.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "5.1",
    "vite": "^4.4.7",
    "vite-bundle-visualizer": "^0.8.0",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-dynamic-import": "^1.5.0",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-plugin-static-copy": "^0.17.0",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.33.0"
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
