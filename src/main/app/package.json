{
  "name": "konfo-ui",
  "version": "0.1.0",
  "homepage": "/konfo",
  "private": true,
  "proxy": "https://beta.hahtuvaopintopolku.fi",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "cypress:run": "./node_modules/.bin/cypress run -b chrome",
    "cypress:run:headless": "npm run cypress:run -- --headless",
    "cypress:open": "./node_modules/.bin/cypress open -b chrome",
    "test:integration": "cross-env CYPRESS_RETRIES=3 NODE_ENV=test REACT_APP_CYPRESS=true PORT=3005 start-server-and-test start http://localhost:3005 cypress:run:headless",
    "test:ci": "cross-env CI=true npm run test && cross-env CI=true npm run test:integration",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.2.5",
    "axios": "^0.19.2",
    "clsx": "^1.1.0",
    "date-fns": "^2.14.0",
    "i18next-http-backend": "^1.0.21",
    "jschannel": "1.0.2",
    "local-storage": "^2.0.0",
    "lodash": "^4.17.20",
    "markdown-to-jsx": "^6.11.4",
    "material-ui-flat-pagination": "^4.1.0",
    "mobx": "^4.3.1",
    "mobx-react": "6.1.4",
    "mobx-react-lite": "^1.5.1",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js#master",
    "oskari-rpc": "^2.1.0",
    "query-string": "^6.11.0",
    "react": "^16.11.0",
    "react-dom": "^16.4.2",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^11.2.1",
    "react-lines-ellipsis": "^0.14.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-select": "^3.0.8",
    "redux": "^4.0.5",
    "remove-markdown": "^0.3.0",
    "striptags": "^3.1.1",
    "superagent": "^3.8.3",
    "typeface-open-sans": "0.0.75",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.8.4",
    "@cypress/snapshot": "^2.1.6",
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@testing-library/cypress": "^7.0.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.2",
    "customize-cra": "0.9.1",
    "cypress": "^5.3.0",
    "cypress-autorecord": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "i18next": "^19.3.4",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-app-rewired": "2.1.5",
    "react-scripts": "^3.4.4",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^1.7.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{json}": [
      "prettier --write",
      "git add"
    ],
    "{src,cypress}/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "cypress/**/*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/react",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "usePrettierrc": true
        }
      ]
    },
    "env": {
      "cypress/globals": true
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^#(.*)$": "<rootDir>$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
