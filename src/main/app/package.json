{
  "name": "konfo-ui",
  "version": "0.1.0",
  "homepage": "/konfo",
  "private": true,
  "scripts": {
    "prepare": "cd ../../../ && husky install",
    "start": "vite dev",
    "build": "vite build",
    "preview:watch": "cross-env vite build --watch & node test-server.js",
    "test": "cross-env NODE_ENV=test vitest",
    "serve:test": "node test-server.js",
    "cypress:run": "cross-env CYPRESS_NO_COMMAND_LOG=1 cypress run -b chrome",
    "cypress:open": "cross-env cypress open -b chrome --e2e",
    "test:integration": "cross-env CYPRESS_RETRIES=3 PORT=3005 start-server-and-test serve:test http://localhost:3005/konfo cypress:run",
    "test:ci": "cross-env CI=true npm run test && cross-env CI=true npm run test:integration",
    "lint": "eslint src/ cypress/",
    "lint:fix": "eslint src/ cypress/ --fix",
    "analyze": "vite-bundle-visualizer",
    "tsc": "tsc"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@mui/styles": "^5.13.2",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.3",
    "express": "^4.18.2",
    "html-react-parser": "^4.0.0",
    "i18next": "^22.5.0",
    "i18next-http-backend": "^2.2.1",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.2.1",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js#9fc4edaa04c10fa974f124b5c228c41c2c6f0bc5",
    "oskari-rpc": "^2.1.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.9",
    "react-i18next": "^12.3.1",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.11.2",
    "react-router-hash-link": "^2.4.3",
    "react-select": "^5.7.3",
    "react-truncate-markup": "^5.1.2",
    "react-use": "^17.4.0",
    "redux": "^4.2.1",
    "remove-markdown": "^0.5.0",
    "stacktrace-js": "^2.0.2",
    "ts-pattern": "^4.3.0",
    "tslib": "^2.5.3",
    "typeface-open-sans": "^1.1.13",
    "victory": "^36.6.10"
  },
  "devDependencies": {
    "@optimize-lodash/rollup-plugin": "^4.0.3",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^14.14.10",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.6",
    "@types/testing-library__cypress": "^5.0.9",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "browserslist-to-esbuild": "^1.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.13.0",
    "cypress-vite": "^1.4.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0",
    "vite": "^4.3.9",
    "vite-bundle-visualizer": "^0.7.0",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.31.4"
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
