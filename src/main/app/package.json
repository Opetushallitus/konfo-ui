{
  "name": "konfo-ui",
  "version": "0.1.0",
  "homepage": "/konfo",
  "private": true,
  "proxy": "https://testiopintopolku.fi",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:test": "cross-env REACT_APP_CYPRESS=true react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "serve:test": "node test-server.js",
    "cypress:run": "cross-env NODE_OPTIONS=\"--max-http-header-size=10000000\" cypress run -b chrome",
    "cypress:run:headless": "npm run cypress:run -- --headless",
    "cypress:open": "cross-env NODE_OPTIONS=\"--max-http-header-size=10000000\" cypress open -b chrome",
    "test:integration": "cross-env CYPRESS_RETRIES=3 NODE_ENV=test REACT_APP_CYPRESS=true PORT=3005 start-server-and-test serve:test http://localhost:3005/konfo cypress:run:headless",
    "test:ci": "cross-env CI=true npm run test && cross-env CI=true npm run test:integration",
    "lint": "eslint src/ cypress/",
    "lint:fix": "eslint src/ cypress/ --fix",
    "update-mocks": "node ./node_modules/kto-ui-common/scripts/update-mocks.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.21.4",
    "clsx": "^1.1.0",
    "date-fns": "^2.14.0",
    "date-fns-tz": "^1.1.6",
    "i18next-http-backend": "^1.0.21",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^6.11.4",
    "material-ui-flat-pagination": "^4.1.0",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js#9fc4edaa04c10fa974f124b5c228c41c2c6f0bc5",
    "oskari-rpc": "^2.1.0",
    "query-string": "^6.11.0",
    "react": "^16.11.0",
    "react-dom": "^16.4.2",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^11.2.1",
    "react-query": "^3.6.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-select": "^3.0.8",
    "react-truncate-markup": "^5.1.0",
    "react-use": "^17.3.1",
    "redux": "^4.0.5",
    "remove-markdown": "^0.3.0",
    "stacktrace-js": "^2.0.2",
    "striptags": "^3.2.0",
    "typeface-open-sans": "0.0.75",
    "url-parse": "^1.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.8.4",
    "@cypress/snapshot": "^2.1.6",
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/markdown-to-jsx": "^6.11.3",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-query": "^1.1.2",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-router-hash-link": "^1.2.1",
    "@types/react-select": "^3.0.27",
    "@types/testing-library__cypress": "^5.0.8",
    "@types/testing-library__jest-dom": "^5.14.1",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.2",
    "customize-cra": "0.9.1",
    "cypress": "^9.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "i18next": "^19.3.4",
    "kto-ui-common": "github:opetushallitus/kto-ui-common#9e49bd2d3c5b956c5cbcb64d8d133276248ff21f",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-app-rewired": "2.1.5",
    "react-scripts": "^4.0.1",
    "source-map-explorer": "^2.5.2",
    "start-server-and-test": "^1.7.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{json}": [
      "prettier --write",
      "git add"
    ],
    "{src,cypress}/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^#(.*)$": "<rootDir>$1"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
