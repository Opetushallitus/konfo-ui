{
  "name": "konfo-ui",
  "version": "0.1.0",
  "homepage": "/konfo",
  "private": true,
  "scripts": {
    "prepare": "cd ../../../ && husky install",
    "start": "vite dev",
    "build": "vite build",
    "test": "cross-env NODE_ENV=test vitest",
    "serve:test": "node test-server.js",
    "cypress:run": "cross-env NODE_OPTIONS=\"--max-http-header-size=10000000\" cypress run -b chrome",
    "cypress:run:headless": "npm run cypress:run -- --headless",
    "cypress:open": "cross-env NODE_OPTIONS=\"--max-http-header-size=10000000\" cypress open -b chrome",
    "test:integration": "cross-env CYPRESS_RETRIES=3 PORT=3005 start-server-and-test serve:test http://localhost:3005/konfo cypress:run:headless",
    "test:ci": "cross-env CI=true npm run test && cross-env CI=true npm run test:integration",
    "lint": "eslint src/ cypress/",
    "lint:fix": "eslint src/ cypress/ --fix",
    "update-mocks": "node ./node_modules/kto-ui-common/scripts/update-mocks.js",
    "analyze": "vite-bundle-visualizer",
    "tsc": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "@mui/styles": "^5.10.6",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "dompurify": "^2.3.6",
    "express": "^4.18.2",
    "html-react-parser": "^3.0.16",
    "i18next": "^21.6.4",
    "i18next-http-backend": "^1.4.1",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.5",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js#9fc4edaa04c10fa974f124b5c228c41c2c6f0bc5",
    "oskari-rpc": "^2.1.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^11.15.2",
    "react-query": "^3.34.7",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.4.4",
    "react-router-hash-link": "^2.4.3",
    "react-select": "^5.2.1",
    "react-truncate-markup": "^5.1.0",
    "react-use": "^17.3.2",
    "redux": "^4.1.2",
    "remove-markdown": "^0.3.0",
    "stacktrace-js": "^2.0.2",
    "ts-pattern": "^4.0.6",
    "tslib": "^2.5.2",
    "typeface-open-sans": "^1.1.13",
    "url-parse": "^1.5.10",
    "victory": "^36.6.8"
  },
  "devDependencies": {
    "@optimize-lodash/rollup-plugin": "^4.0.3",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-router-hash-link": "^2.4.4",
    "@types/testing-library__cypress": "^5.0.9",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react": "^4.0.0",
    "browserslist-to-esbuild": "^1.2.0",
    "cosmiconfig": "^7.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.2.0",
    "cypress-vite": "^1.4.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vite-bundle-visualizer": "^0.7.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.31.1"
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
