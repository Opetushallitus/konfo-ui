{
  "name": "konfo-ui",
  "version": "0.1.0",
  "homepage": "/konfo",
  "private": true,
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@contentful/rich-text-types": "^13.4.0",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "ajv": "^6.5.2",
    "contentful": "^3.5.0",
    "csvtojson": "^2.0.10",
    "i18next": "^11.6.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-xhr-backend": "^1.5.1",
    "jschannel": "1.0.2",
    "lodash": "^4.17.15",
    "markdown-to-jsx": "^6.10.3",
    "material-icons": "^0.3.1",
    "mobx": "^4.3.1",
    "mobx-react": "6.1.4",
    "mobx-react-lite": "^1.5.1",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js#master",
    "oskari-rpc": "2.0.5",
    "react": "^16.11.0",
    "react-dom": "^16.4.2",
    "react-i18next": "^11.2.1",
    "react-media": "^1.8.0",
    "react-render-html": "0.6.0",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-select": "^3.0.8",
    "remove-markdown": "^0.3.0",
    "superagent": "^3.8.3",
    "url-parse": "^1.4.7"
  },
  "config": {
    "port": "3005",
    "backendport": "3006",
    "testport": "3482",
    "testbackendport": "4483"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ]
  },
  "scripts": {
    "build": "react-app-rewired build",
    "test": "./scripts/test.sh $PORT $BACKEND_PORT",
    "start-for-start": "react-app-rewired start",
    "start": "./scripts/start.sh $PORT $BACKEND_PORT",
    "start-for-test": "BROWSER=none NODE_ENV=test react-app-rewired start",
    "start-and-test": "start-test start-for-test $PORT run-test",
    "run-test": "react-app-rewired test --env=jsdom",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "devDependencies": {
    "customize-cra": "0.9.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "puppeteer": "^1.7.0",
    "react-app-rewired": "2.1.5",
    "react-scripts": "3.2.0",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/mocks"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
